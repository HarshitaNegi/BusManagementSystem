<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="model.schedule" table="allschedule">
        <id name="id" type="java.lang.Integer"></id>
        <property name="busno" column="busno"></property>
        <property name="pickuppoint" column="pickuppoint"></property>
        <property name="arrivaltime" column="arrivaltime"></property>
        <property name="droppoint" column="droppoint"></property>
        <property name="droptime" column="droptime"></property>
    </class>
</hibernate-mapping>
